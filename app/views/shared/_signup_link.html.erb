
		<% if current_user %>
				<div id="user_nav">
						Logged in as <b><%=  current_user.login.capitalize %></b>, 
						<%= link_to 'LogOut', logout_path %>
				</div>
		<% else %>
				<div id="user_nav">
						<%= link_to "Register", new_user_path %> | 
						<%= link_to "Login", login_path %>
				</div>
		<% end %>


